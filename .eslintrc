{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "extends": "eslint:recommended",
  "env": {
    "jest": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "consistent-return": 0,
    "no-use-before-define": 0,
    "padded-blocks": 0,
    "object-curly-spacing": ["error", "never"],
    "no-console": ["error", {allow: ["error"]}]
  },
  "plugins": [
    "import"
  ]
}
